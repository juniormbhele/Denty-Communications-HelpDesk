<?xml version="1.0" encoding="UTF-8"?>
<project name="module_denty_communications_-_helpdesk" default="compile.module.denty_communications_-_helpdesk">
  <dirname property="module.denty_communications_-_helpdesk.basedir" file="${ant.file.module_denty_communications_-_helpdesk}"/>
  
  <property name="module.jdk.home.denty_communications_-_helpdesk" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.denty_communications_-_helpdesk" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.denty_communications_-_helpdesk" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.denty_communications_-_helpdesk" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="denty_communications_-_helpdesk.output.dir" value="${module.denty_communications_-_helpdesk.basedir}/out/production/Denty Communications - HelpDesk"/>
  <property name="denty_communications_-_helpdesk.testoutput.dir" value="${module.denty_communications_-_helpdesk.basedir}/out/test/Denty Communications - HelpDesk"/>
  
  <path id="denty_communications_-_helpdesk.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="denty_communications_-_helpdesk.module.production.classpath">
    <path refid="${module.jdk.classpath.denty_communications_-_helpdesk}"/>
    <path refid="library.glassfish_4.1.2.classpath"/>
    <pathelement location="${idea.home}/lib/groovy-all-2.2.1.jar"/>
    <pathelement location="${idea.home}/plugins/testng/lib/testng.jar"/>
  </path>
  
  <path id="denty_communications_-_helpdesk.runtime.production.module.classpath">
    <pathelement location="${denty_communications_-_helpdesk.output.dir}"/>
    <pathelement location="${idea.home}/lib/groovy-all-2.2.1.jar"/>
    <pathelement location="${idea.home}/plugins/testng/lib/testng.jar"/>
  </path>
  
  <path id="denty_communications_-_helpdesk.module.classpath">
    <path refid="${module.jdk.classpath.denty_communications_-_helpdesk}"/>
    <pathelement location="${denty_communications_-_helpdesk.output.dir}"/>
    <path refid="library.glassfish_4.1.2.classpath"/>
    <pathelement location="${idea.home}/lib/groovy-all-2.2.1.jar"/>
    <pathelement location="${idea.home}/plugins/testng/lib/testng.jar"/>
  </path>
  
  <path id="denty_communications_-_helpdesk.runtime.module.classpath">
    <pathelement location="${denty_communications_-_helpdesk.testoutput.dir}"/>
    <pathelement location="${denty_communications_-_helpdesk.output.dir}"/>
    <path refid="library.glassfish_4.1.2.classpath"/>
    <pathelement location="${idea.home}/lib/groovy-all-2.2.1.jar"/>
    <pathelement location="${idea.home}/plugins/testng/lib/testng.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.denty_communications_-_helpdesk">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.denty_communications_-_helpdesk">
    <patternset refid="excluded.from.module.denty_communications_-_helpdesk"/>
  </patternset>
  
  <path id="denty_communications_-_helpdesk.module.sourcepath">
    <dirset dir="${module.denty_communications_-_helpdesk.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.denty_communications_-_helpdesk" depends="compile.module.denty_communications_-_helpdesk.production,compile.module.denty_communications_-_helpdesk.tests" description="Compile module Denty Communications - HelpDesk"/>
  
  <target name="compile.module.denty_communications_-_helpdesk.production" depends="register.custom.compilers" description="Compile module Denty Communications - HelpDesk; production classes">
    <mkdir dir="${denty_communications_-_helpdesk.output.dir}"/>
    <javac2 destdir="${denty_communications_-_helpdesk.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.denty_communications_-_helpdesk}/javac">
      <compilerarg line="${compiler.args.denty_communications_-_helpdesk}"/>
      <bootclasspath refid="denty_communications_-_helpdesk.module.bootclasspath"/>
      <classpath refid="denty_communications_-_helpdesk.module.production.classpath"/>
      <src refid="denty_communications_-_helpdesk.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.denty_communications_-_helpdesk"/>
    </javac2>
    
    <copy todir="${denty_communications_-_helpdesk.output.dir}">
      <fileset dir="${module.denty_communications_-_helpdesk.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.denty_communications_-_helpdesk.tests" depends="register.custom.compilers,compile.module.denty_communications_-_helpdesk.production" description="compile module Denty Communications - HelpDesk; test classes" unless="skip.tests"/>
  
  <target name="clean.module.denty_communications_-_helpdesk" description="cleanup module">
    <delete dir="${denty_communications_-_helpdesk.output.dir}"/>
    <delete dir="${denty_communications_-_helpdesk.testoutput.dir}"/>
  </target>
</project>